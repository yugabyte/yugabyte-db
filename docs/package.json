{
  "name": "docs",
  "version": "1.3.0",
  "description": "Yugabyte Docs",
  "main": "public/index.html",
  "scripts": {
    "hugo-dependencies": "node scripts/install.js && node scripts/dependencies.js",
    "start": "npm run hugo-dependencies && run-p start-diagrams webpack-watch hugo-server",
    "fast": "npm run hugo-dependencies && run-p webpack-watch hugo-server-fast",
    "start-production": "npm run hugo-dependencies && run-p webpack-watch hugo-server-production",
    "start-static": "npm run hugo-dependencies && run-p webpack-watch hugo-server-static",
    "build": "rm -rf public && npm ci && npm run hugo-dependencies && run-p start-diagrams hugo-base-build",
    "hugo-base-build": "run-s webpack-build hugo-build stop-diagrams",
    "debug-build": "rm -rf public && npm ci && npm run hugo-dependencies && run-p start-diagrams hugo-base-debug-build",
    "hugo-base-debug-build": "run-s webpack-build hugo-build-with-metrics stop-diagrams",
    "hugo-build": "hugo -b https://docs.yugabyte.com",
    "hugo-build-with-metrics": "hugo --templateMetrics --templateMetricsHints -b https://docs.yugabyte.com",
    "hugo-server": "hugo server --bind 0.0.0.0 --baseURL ${YB_HUGO_BASE:-localhost}",
    "hugo-server-fast": "hugo server --bind 0.0.0.0 --baseURL ${YB_HUGO_BASE:-localhost} --environment fast",
    "hugo-server-production": "hugo server --bind 0.0.0.0 --baseURL ${YB_HUGO_BASE:-localhost} --environment production",
    "hugo-server-static": "hugo server --renderStaticToDisk --bind 0.0.0.0 --baseURL ${YB_HUGO_BASE:-localhost}",
    "start-diagrams": "java -jar node_modules/yb-rrdiagram/rrdiagram*.jar --server",
    "stop-diagrams": "curl --silent http://localhost:1314/shutdown",
    "webpack-build": "webpack --mode=production",
    "webpack-watch": "webpack --mode=production -w"
  },
  "author": "YugabyteDB, Inc.",
  "license": "Apache License 2.0",
  "homepage": "https://github.com/yugabyte/yugabyte-db/docs#readme",
  "dependencies": {
    "@babel/core": "7.28.6",
    "@babel/plugin-proposal-decorators": "7.28.6",
    "@babel/preset-env": "7.28.6",
    "@stylistic/eslint-plugin": "5.7.0",
    "algoliasearch": "4.23.3",
    "browser-cookie-utils": "2.0.1",
    "browser-query-utils": "2.0.0",
    "babel-loader": "10.0.0",
    "clipboard": "2.0.11",
    "detect-external-link": "2.0.1",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-webpack-plugin": "5.0.2",
    "globals": "17.0.0",
    "sass": "1.97.2",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3",
    "yb-rrdiagram": "0.0.7"
  },
  "devDependencies": {
    "autoprefixer": "10.4.23",
    "dotenv": "17.2.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "run-p": "0.0.0"
  }
}
