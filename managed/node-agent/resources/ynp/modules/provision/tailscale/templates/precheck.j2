# Check if tailscale binary exists
if command -v tailscale &>/dev/null; then
  tailscale_version=$(tailscale version | head -n1 | awk '{print $1}')
  echo "Tailscale is installed: $tailscale_version"
  add_result "Tailscale Binary Check" "PASS" \
    "Tailscale is installed: $tailscale_version"
else
  echo "Tailscale is not installed"
  add_result "Tailscale Binary Check" "FAIL" "Tailscale is not installed"
fi
