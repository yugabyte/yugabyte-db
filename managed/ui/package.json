{
  "name": "YB-Customer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write src/redesign/**/*",
    "cleanup": "rimraf build && rimraf ../src/main/public",
    "fetch-map": "mkdir -p build/static && curl 'http://s3-us-west-2.amazonaws.com/map.yugabyte.com/map/yb_map.tar.gz' | tar -zx -C build/static",
    "build": "npm run cleanup && CI=false REACT_APP_GIT_COMMIT=$(git rev-parse HEAD) craco build && npm run fetch-map",
    "build:ci": "CI=true GENERATE_SOURCEMAP=false REACT_APP_GIT_COMMIT=$(git rev-parse HEAD) craco build && npm run fetch-map",
    "build-and-copy": "npm run build:ci && ncp build ../src/main/public",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "test:ci": "CI=true craco test --env=jest-environment-jsdom-sixteen",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --env=jest-environment-jsdom-sixteen",
    "start": "craco start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -s public",
    "generateV2JSApiClient": "[ -f ../src/main/resources/openapi.yaml ] && orval --config ./v2Api.config.ts"
  },
  "engines": {
    "node": ">=16.10.x",
    "npm": ">=7.24.x"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@craco/craco": "6.4.5",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@testing-library/dom": "7.29.6",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.0",
    "@types/js-yaml": "4.0.5",
    "@types/prismjs": "1.26.0",
    "@types/react-beautiful-dnd": "13.1.3",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "ajv": "8.16.0",
    "esbuild-loader": "^2.20.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-junit": "12.0.0",
    "ncp": "2.0.0",
    "orval": "6.4.1",
    "prettier": "2.1.0",
    "rimraf": "2.7.1",
    "sass": "1.52.1",
    "source-map-explorer": "2.5.0",
    "storybook": "^7.0.27"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@types/lodash": "4.14.159",
    "@types/pluralize": "0.0.29",
