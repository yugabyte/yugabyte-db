msgstr "  \\dg[S+] [PATTERN]      롤 목록\n"

#: help.c:271
msgid "  \\di[S+] [PATTERN]      list indexes\n"
msgstr "  \\di[S+] [PATTERN]      인덱스 목록\n"

#: help.c:272
