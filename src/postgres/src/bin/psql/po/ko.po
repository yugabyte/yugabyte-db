"\n"

#: help.c:479
#, c-format
msgid ""
"  set NAME=VALUE\n"
"  psql ...\n"
"  or \\setenv NAME [VALUE] inside psql\n"
"\n"
msgstr ""
"  set NAME=VALUE\n"
"  psql ...\n"
"  또는 psql 명령 모드에서는 \\setenv NAME [VALUE]\n"
"\n"

#: help.c:482
#, c-format
