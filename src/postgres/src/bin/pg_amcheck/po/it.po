msgstr "  -e, --echo                      mostra i comandi inviati al server\n"

#: pg_amcheck.c:1175
#, c-format
msgid "  -j, --jobs=NUM                  use this many concurrent connections to the server\n"
msgstr " -j, --jobs=NUM usa questo numero di connessioni simultanee al server\n"

