msgid "too many command-line arguments (first is \"%s\")"
msgstr "забагато аргументів у командному рядку (перший \"%s\")"

#: pg_waldump.c:970 pg_waldump.c:990
#, c-format
msgid "could not open directory \"%s\": %m"
