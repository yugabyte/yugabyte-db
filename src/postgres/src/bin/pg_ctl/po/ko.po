#, c-format
msgid "invalid binary \"%s\""
msgstr "잘못된 바이너리 파일 \"%s\""

#: ../../common/exec.c:206
#, c-format
msgid "could not read binary \"%s\""
msgstr "\"%s\" 바이너리 파일을 읽을 수 없음"

