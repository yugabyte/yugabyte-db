         ->  Hash
               ->  Hash Semi Join
                     Hash Cond: (t2.b2 = "ANY_subquery_1".a5)
                     ->  Nested Loop
                           ->  Hash Semi Join
                                 Hash Cond: (t1.b1 = "ANY_subquery".a4)
                                 ->  Seq Scan on t1
                                 ->  Hash
                                       ->  Subquery Scan on "ANY_subquery"
                                             ->  HashAggregate
                                                   Group Key: t4.a4, t4.b4
                                                   ->  Seq Scan on t4
                           ->  Memoize
                                 Cache Key: t1.a1
                                 Cache Mode: logical
                                 ->  Index Scan using t2_a2_idx on t2
                                       Index Cond: (a2 = t1.a1)
                     ->  Hash
                           ->  Subquery Scan on "ANY_subquery_1"
                                 ->  HashAggregate
