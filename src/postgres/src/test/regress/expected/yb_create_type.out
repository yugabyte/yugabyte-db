-- Borrowed from https://www.postgresql.org/docs/11/rangetypes.html#RANGETYPES-BUILTIN
-- Containment
SELECT int4range(10, 20) @> 3;
 ?column?
----------
 f
(1 row)

-- Overlaps
SELECT numrange(11.1, 22.2) && numrange(20.0, 30.0);
