CREATE TYPE bogus AS ENUM('good');
ALTER TYPE bogus RENAME TO bogon;
SELECT 'good'::bogus; -- fail
ERROR:  type "bogus" does not exist
LINE 1: SELECT 'good'::bogus;
                       ^
SELECT 'good'::bogon;
 bogon
-------
