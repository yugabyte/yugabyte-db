-- JOIN Test Cases
-- Table Join
SELECT
				t1.col_smallint,
				t1.col_array_text,
				t2.col_name
		FROM feature_tab_dml t1, feature_tab_dml_identifier t2
		WHERE t1.col_smallint = t2.col_id AND t2.col_name = t1.col_array_text[2]
        ORDER BY t1.col_smallint;
 col_smallint |   col_array_text    | col_name
