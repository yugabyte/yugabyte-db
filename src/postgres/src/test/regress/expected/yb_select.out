   ->  Aggregate
         ->  YB Bitmap Table Scan on address a
               ->  Bitmap Index Scan on idx_address_problem
                     Index Cond: (site_id = 1)
                     Storage Index Filter: (recorded_at IS NULL)
   ->  Seq Scan on site s
(7 rows)

