--
-- For https://github.com/YugaByte/yugabyte-db/issues/20316
--
create table a (a int);
insert into a values (1),(2),(3),(4);
create table b (b int, a int);
insert into b values (1, 1),(2, 1),(3, 2),(4, 4);
create table c (c int);
insert into c values (1),(2),(3);
create table d (b int, c int);
insert into d values (1, 1),(1, 2),(1, 3),(2, 1),(3, 2),(3, 3);
explain (verbose, costs off)
select
  a.a,
  (
    select sum(t.v0)
    from (
      select c.c as v0
      from c
      where c.c in (
        select d.c
        from d
          join b
            on b.b = d.b
        where b.a = a.a
      )
    ) as t
  )
from a
order by a.a;
                                   QUERY PLAN
---------------------------------------------------------------------------------
 Sort
   Output: a.a, ((SubPlan 1))
   Sort Key: a.a
   ->  Seq Scan on public.a
         Output: a.a, (SubPlan 1)
         SubPlan 1
           ->  Aggregate
                 Output: sum(c.c)
                 ->  Hash Join
                       Output: c.c
                       Inner Unique: true
                       Hash Cond: (c.c = d.c)
                       ->  Seq Scan on public.c
                             Output: c.c
                       ->  Hash
                             Output: d.c
                             ->  HashAggregate
                                   Output: d.c
                                   Group Key: d.c
                                   ->  Merge Join
                                         Output: d.c
                                         Merge Cond: (d.b = b.b)
                                         ->  Sort
                                               Output: d.b, d.c
                                               Sort Key: d.b
                                               ->  Seq Scan on public.d
                                                     Output: d.b, d.c
                                         ->  Sort
                                               Output: b.b
                                               Sort Key: b.b
                                               ->  Seq Scan on public.b
                                                     Output: b.b
                                                     Storage Filter: (b.a = a.a)
(33 rows)

select
  a.a,
  (
    select sum(t.v0)
    from (
      select c.c as v0
      from c
      where c.c in (
        select d.c
        from d
          join b
            on b.b = d.b
        where b.a = a.a
      )
    ) as t
  )
from a
order by a.a;
