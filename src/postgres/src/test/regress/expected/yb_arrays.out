   FROM arrtest ORDER BY c[1];
 a  | b |   c    |  d   |  e
----+---+--------+------+-----
    |   | foo    |      |
 11 |   | foobar | elt1 |
  1 | 0 |        |      | 1.1
(3 rows)

SELECT a[1], b[1][1][1], c[1], d[1][1], e[0]
   FROM arrtest ORDER BY c[1];
 a  | b |   c    |  d   |  e
----+---+--------+------+-----
    |   | foo    |      |
