CREATE EXTENSION ltree_plpython3u CASCADE;
NOTICE:  installing required extension "plpython3u"
CREATE FUNCTION test1(val ltree) RETURNS int
LANGUAGE plpython3u
TRANSFORM FOR TYPE ltree
AS $$
plpy.info(repr(val))
return len(val)
$$;
SELECT test1('aa.bb.cc'::ltree);
INFO:  ['aa', 'bb', 'cc']
 test1 
-------
     3
(1 row)

