{
  "name": "yugabyted-ui-application",
  "version": "0.0.1",
  "scripts": {
    "start": "vite",
    "build": "rm -rf ui && NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "generate-api-client": "openapi-generator generate -i $([ -f ./openapi.yml ] && echo './openapi.yml' || echo '../apiserver/conf/openapi.yml') -g typescript-react-query -o ./src/api"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "overrides": {
    "react-dnd": "$react-dnd",
    "react-dnd-html5-backend": "$react-dnd-html5-backend",
    "dnd-core": "14.0.1"
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@hookform/resolvers": "2.9.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/styles": "4.11.4",
    "@material-ui/system": "4.12.1",
    "axios": "0.21.4",
    "axios-mock-adapter": "1.19.0",
    "brace": "0.11.1",
    "clsx": "1.1.1",
    "cron-parser": "4.0.0",
    "date-fns": "2.27.0",
    "i18next": "21.6.16",
    "immutability-helper": "3.1.1",
    "ip-address": "8.1.0",
    "material-ui-pickers-v4": "3.3.12",
    "mui-datatables": "3.8.5",
    "openapi": "1.0.1",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-ace": "9.4.4",
    "react-dnd": "14.0.4",
    "react-dnd-html5-backend": "14.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.0",
    "react-i18next": "11.16.6",
    "react-joyride": "3.0.0-7",
    "react-query": "3.32.1",
    "react-query-openapi-generator": "0.0.12",
    "react-router-dom": "5.3.3",
    "react-use": "17.2.4",
    "recharts": "2.0.9",
    "use-query-params": "1.2.3",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "6.5.1",
    "@svgr/plugin-svgo": "6.5.1",
    "@types/mui-datatables": "3.7.4",
    "@types/node": "18.14.0",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "3.1.0",
    "typescript": "5.0.4",
    "vite": "4.5.5",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-svgr": "2.4.0"
  }
}
